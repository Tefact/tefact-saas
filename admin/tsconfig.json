{
  "compilerOptions": {
    "declaration": false,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "outDir": "./dist",
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "target": "es2020",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "esnext",
      "es2015",
      "esnext.asynciterable",
      "dom"
    ],
    "esModuleInterop": true,
    "allowJs": false,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ],
      "lodash/*": [
        "node_modules/@types/lodash-es/*"
      ]
    },
    "types": [
      "@types/node",
      "@nuxt/types",
      "@nuxt/vue-app"
    ]
  },
  "include": ["**/*.ts", "**/*.vue"],
  "exclude": [
    "node_modules"
  ],
  "files": ["ts-shim.d.ts"]
}
